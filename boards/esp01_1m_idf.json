{
  "build": {
    "core": "esp8266",
    "extra_flags": "-DESP8266 -DARDUINO_ARCH_ESP8266 -DARDUINO_ESP8266_ESP01",
    "f_cpu": "80000000L",
    "f_flash": "40000000L",
    "flash_mode": "dio",
    "mcu": "esp8266",
    "variant": "generic"
  },
  "connectivity": [
    "wifi"
  ],
  "frameworks": [
    "esp8266-rtos-idf"
  ],
  "name": "Espressif Generic ESP8266 ESP-01 1M (IDF)",
  "upload": {
    "flash_size": "1MB",
    "maximum_ram_size": 81920,
    "maximum_size": 1048576,
    "require_upload_port": true,
    "resetmethod": "ck",
    "speed": 115200,
    "protocol": "esptool"
  },
  "url": "http://www.esp8266.com/wiki/doku.php?id=esp8266-module-family",
  "vendor": "Espressif"
}
